                {
                    "command": ":tabnext",
                    "args": []
                }
            ]
        },
        {
            "before": ["<C-h>"],
            "after": ["<C-w>", "h"],
        },
        {
            "before": ["<C-j>"],
            "after": ["<C-w>", "j"],
        },
        {
            "before": ["<C-k>"],
            "after": ["<C-w>", "k"],
        },
        {
            "before": ["<C-l>"],
            "after": ["<C-w>", "l"],
        },
        {
            "before": ["<C-u>"],
            "after": ["dd", "k", "P"],
        }
    ],
