    "editor.tokenColorCustomizations": {
        "[Gruvbox Dark (Medium)]": {
            "textMateRules": [
                {
                    "scope": ["keyword.control.import", "keyword.control.from"],
                    "settings": {
                        "foreground": "#8ec07c"
                    }
                },
                {
                    "scope": ["variable", "variable.other.constant"],
                    "settings": {
                        "foreground": "#ebdbb2"
                    }
                },
                {
                    "scope": "support.type",
                    "settings": {
                        "foreground": "#d3869b",
                    }
                },
                {
                    "scope": "entity.name.type",
                    "settings": {
                        "foreground": "#8ec07c",
                    }
                }
            ]
        },
    },
    "workbench.iconTheme": "material-icon-theme",
    "editor.fontFamily": "Source Code Pro",
    "editor.tabSize": 2,
    "terminal.integrated.fontFamily": "Anonymous Pro for Powerline",
    "terminal.integrated.fontSize": 14,
    "editor.smoothScrolling": true,
    "editor.tabCompletion": "on",
    "editor.cursorSmoothCaretAnimation": true,
    "editor.formatOnSave": true,
    "files.defaultLanguage": "javascript",
    "workbench.commandPalette.history": 10000,
    "search.exclude": {
        "**/.git": true
    },
    "terminal.external.osxExec": "iTerm.app",
    "terminal.integrated.scrollback": 10000,
    "css.lint.duplicateProperties": "warning",
    "css.lint.important": "error",
    "css.lint.zeroUnits": "warning",
    "editor.fontWeight": "600",
    "editor.fontSize": 13,
    "terminal.integrated.fontWeight": "600",
    "javascript.preferences.quoteStyle": "single",
    "typescript.preferences.quoteStyle": "single",
    "editor.renderWhitespace": "boundary",
    "vim.foldfix": true,
    "[typescriptreact]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "[typescript]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "editor.suggestSelection": "first",
    "vsintellicode.modify.editor.suggestSelection": "automaticallyOverrodeDefaultValue",
    "editor.minimap.enabled": false,
    "files.exclude": {
        "**/.classpath": true,
        "**/.project": true,
        "**/.settings": true,
        "**/.factorypath": true
    },
    "java.errors.incompleteClasspath.severity": "ignore",
    "typescript.updateImportsOnFileMove.enabled": "never",
    "window.zoomLevel": 0,
    "sync.autoUpload": true,
    "sync.autoDownload": true,
    "sync.gist": "89a1e6e08b257bcf7378715662e2ebe9",
    "python.jediEnabled": false
